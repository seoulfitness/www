                </foreach>
                <foreach collection="gradeRecordMaxs" index="index" item="max">
                    <if test="max != null">
                        <![CDATA[
                        GRADE${index + 1}_RECORD_MAX = #{max},
                        ]]>
                    </if>
                </foreach>
            </trim>
        </if>
        <![CDATA[
        WHERE 1=1
        ]]>
        <include refid="commonWhereConditions" />
        <![CDATA[
